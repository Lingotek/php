{
  "name": "lingotek/php-sdk",
  "type": "library",
  "description": "The Lingotek PHP API Client",
  "keywords": ["multilingual", "php", "rest"],
  "homepage": "https://github.com/lingotek/php",
  "license": ["BSD 3-Clause"],
  "authors": [
    {
      "name": "Lingotek",
      "homepage": "http://www.lingotek.com"
    },
    {
      "name": "Matt Smith",
      "email": "msmith@lingotek.com",
      "homepage": "https://github.com/smithworx"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*"
  },
  "autoload": {
    "psr-4": {
      "Lingotek\\": "src/"
    }
  },
  "autoload-dev" : {
    "psr-4": {
      "Lingotek\\Dev\\": "examples/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
